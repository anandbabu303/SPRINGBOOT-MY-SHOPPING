<script>
  window.onload = function () {
    const jcontainer = document.querySelector('.j-carousel-slides');
    const slides = document.querySelectorAll('.j-slide');
    const dots = document.querySelectorAll('.j-dot');

    jcontainer.addEventListener('scroll', updateDots);
    dots.forEach((dot, index) => {
      dot.addEventListener('click', () => {
        jcontainer.scroll({
          left: index * jcontainer.clientWidth,
          behavior: 'smooth',
        });
      });
    });
    function updateDots() {
      let index = Math.round(jcontainer.scrollLeft / jcontainer.clientWidth);
      dots.forEach((dot) => dot.classList.remove('active'));
      dots[index].classList.add('active');
    }

    // Initialize the first dot as active
    dots[0].classList.add('active');
  };
</script>

  
